{
  "variables": {

  },
  "builders": [
    {
      "type": "vagrant",
      "communicator": "ssh",
      "source_path": "hashicorp/bionic64",
      "provider": "virtualbox",
      "add_force": false,
      "skip_add": true
    }
  ],
  "provisioners": [
    {
      "type": "file",
      "source": "./install",
      "destination": "/tmp/install"
    },
    {
      "type": "shell",
      "inline": [
        "sudo make -C /tmp/install install test"
      ]
    }
  ],
  "post-processors": [
    
  ]
}
